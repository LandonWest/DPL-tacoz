<h1>Admin Section - LOCATIONS-INDEX</h1>

<% @locations.each do |l| %>
  <ul>
    <li>
      <ul>
        <li><%= l.street_address %></li>
        <li><%= l.street_address2 %></li>
        <li><%= l.city %></li>
        <li><%= l.state %></li>
        <li><%= l.zip %></li>
        <li><%= l.latitude %></li>
        <li><%= l.longitude %></li>
        <%= link_to 'Show', admin_location_path(l.id) %>
        
      </ul>
    </li>
  </ul>
<% end %>
